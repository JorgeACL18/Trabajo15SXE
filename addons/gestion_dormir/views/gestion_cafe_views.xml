<odoo>
    <record id="tree_cafe" model="ir.ui.view">
        <field name="name">gestion.dormir.list</field>
        <field name="model">gestion.dormir</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="sleepy"/>
                <field name="coffee"/>
            </list>
        </field>
    </record>

    <record id="form_cafe" model="ir.ui.view">
        <field name="name">gestion.dormir.form</field>
        <field name="model">gestion.dormir</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="sleepy"/>
                        <field name="coffee"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_cafe" model="ir.actions.act_window">
        <field name="name">Recomendador de Bebidas</field>
        <field name="res_model">gestion.dormir</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="root_cafe" name="Control de SueÃ±o" sequence="10"/>
    <menuitem id="main_cafe" name="Alumnos" parent="root_cafe" action="action_cafe" sequence="10"/>
</odoo>